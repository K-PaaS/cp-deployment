image:
  repository: registry.k-paas.org/k8s/nfs-subdir-external-provisioner

nfs:
  server: {NFS_SERVER_PRIVATE_IP}
  path: /home/share/nfs

storageClass:
  provisionerName: cp-nfs-provisioner
  defaultClass: true
  name: cp-storageclass
  allowVolumeExpansion: false
  reclaimPolicy: Retain
  archiveOnDelete: false
